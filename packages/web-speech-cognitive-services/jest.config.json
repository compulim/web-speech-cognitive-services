{
  "setupFilesAfterEnv": ["./utils/setupDotEnv.js", "./utils/setupGlobalAgent.js", "./utils/setupFetchPolyfill.js"],
  "testPathIgnorePatterns": ["/__setup__/", "/lib/", "/node_modules/", "/__types__/", "\\.pnp\\.[^\\/]+$"],
  "transform": {
    "/__tests__/types/": ["<rootDir>/__tests__/__setup__/typingTestTransformer.js"],
    "\\.[jt]sx?$": [
      "babel-jest",
      {
        "presets": [
          [
            "@babel/preset-typescript",
            {
              "allowDeclareFields": true
            }
          ],
          [
            "@babel/preset-env",
            {
              "modules": "commonjs",
              "targets": {
                "node": "18"
              }
            }
          ]
        ],
        "sourceMaps": true
      }
    ]
  },
  "transformIgnorePatterns": ["/node_modules/(?!(p-defer))/"],
  "watchPathIgnorePatterns": ["\\.tmp\\."]
}
